let arr = [7, 1, 5, 4, 3, 2];

function selection_sort(nums) {
    for (let i = 0; i < nums.length; i++) {
        let min = i;
        for (let j = i + 1; j < nums.length; j++) {
            if (nums[j] < nums[min]) {
                min = j;
            }
        }
        if (min != i) {
            let temp = nums[min];
            nums[min] = nums[i];
            nums[i] = temp;
        }
    }
    return nums;
}

const res = selection_sort(arr);
console.log(res);
